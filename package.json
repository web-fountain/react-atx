{
  "name": "reactjs-atx",
  "version": "0.1.0",
  "description": "reactjs atx website",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/reactjs-atx/website.git"
  },
  "keywords": [
    "atx",
    "community",
    "react",
    "website"
  ],
  "author": "Roberto Fuentes",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/reactjs-atx/website/-/issues"
  },
  "homepage": "https://gitlab.com/reactjs-atx/website/-/tree/main#reactjs-atx",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.12.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "babel": "npm run clean:babel && babel --config-file=./.configs/babelrc.json --extensions='.js,.ts,.tsx' --out-dir=_babel-output src",
    "build": "npm run clean:webpack && webpack --config ./.configs/webpack/index.ts --mode=production",
    "build:run": "npm run clean:webpack && webpack --config ./.configs/webpack/index.ts --mode=production && http-server ./_webpack-output -p 3000",
    "clean": "rimraf _babel-output _eslint-output _typescript-output _webpack-output",
    "clean:babel": "rimraf _babel-output",
    "clean:eslint": "rimraf _eslint-output",
    "clean:typescript": "rimraf _typescript-output",
    "clean:webpack": "rimraf _webpack-output",
    "eslint": "npm run clean:eslint && eslint .configs --config ./.configs/eslintrc.json --no-color -o _eslint-output/report",
    "repack": "rm -rf node_modules/ package-lock.json && npm install",
    "start": "webpack serve --config ./.configs/webpack/index.ts --mode=development",
    "run": "http-server ./_webpack-output -p 3000",
    "typescript": "npm run clean:typescript && tsc --project ./.configs/tsconfig.json",
    "react": "http-server ./src -p 8000"
  }
}
