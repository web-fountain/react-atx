// Plugins run before Presets
// Plugin ordering is first to last
// Preset ordering is reversed (last to first)


{
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": 3,
      "version": "^7.14.7",
      "regenerator": true
    }],
    "@babel/plugin-syntax-dynamic-import" // don't really need this now -- will remove
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}

